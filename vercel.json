{
    "version": 2,
    "builds": [
        {
            "src": "web.html",
            "use": "@vercel/static"
        },
        {
            "src": "projects-python.html",
            "use": "@vercel/static"
        },
        {
            "src": "projects-java.html",
            "use": "@vercel/static"
        },
        {
            "src": "projects-cpp.html",
            "use": "@vercel/static"
        },
        {
            "src": "contact.html",
            "use": "@vercel/static"
        },
        {
            "src": "script.js",
            "use": "@vercel/static"
        },
        {
            "src": "python/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "java/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "handle": "filesystem"
        }
    ]
}
